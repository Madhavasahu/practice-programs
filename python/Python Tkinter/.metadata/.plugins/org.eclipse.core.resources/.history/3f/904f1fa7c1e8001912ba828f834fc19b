import tkinter as tk
from tkinter import messagebox, Toplevel, Frame

def main():
    root = tk.Tk()
    root.wm_title("Main Window")
    root.geometry("1366x768")
    messagebox.showwarning("msg Box","tkitner message box")
    t1=Toplevel(root)
    t1.geometry("500x500")
    tk.Label(t1, text='This is a child of root').pack(padx=10, pady=10)
    f=Frame(t1).pack()
    tk.Label(f,text="hello").pack(side=leftpadx=50, pady=10)
    root.mainloop()
if __name__ == "__main__":
    main()
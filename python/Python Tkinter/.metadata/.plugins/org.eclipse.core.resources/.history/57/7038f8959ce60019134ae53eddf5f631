from tkinter import *

root = Tk()
windWidth = 1378
windHeight = 768
screenWidth = root.winfo_screenmmwidth()
screenHeight =root.winfo_screenmmheight()

x = (screenWidth/2)-(screenWidth/2)
y = (screenWidth/2)-(screenWidth/2)
root.geometry("%dx%d+%d+%d" % (windWidth,windHeight,screenWidth,screenHeight))


root.mainloop()
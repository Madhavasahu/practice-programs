from tkinter import *

def switch():
    b1["state"] = DISABLED

root = Tk()
root.wm_title("Main Window")
root.geometry("500x500")
f=Frame(root,height = 100,width=100)
g=Frame(root,height=10,width=50,relief=GROOVE,borderwidth=2)
L=Label(g,text="hi").pack(padx=19)
B1=Button(g,text="button1",command=switch).pack(padx=50,pady=50)
B1["state"]=DISABLED
Button(g,text="button2").pack(padx=40,pady=50)
Label(f,text="Hello").pack(pady=10)
f.pack()
g.pack()



    root.mainloop()
